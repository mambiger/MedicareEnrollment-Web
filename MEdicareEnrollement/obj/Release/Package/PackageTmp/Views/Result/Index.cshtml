
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MedicareLayout.cshtml";
}

<div class="main-content">
    <div class="container">
        <div class="caption-heading">
            <div class="caption-h">
                Enrollment
            </div>
        </div>

    </div>



    <div class="container">
        <div class="form-head">
            Enrollment Status
        </div>

        <div class="container">
            <div class="col-md-12 col-sm-12">
                <form class="form-horizontal" role="form">
                    @{  if (ViewBag.errordata == "")
                        {
                            <div class="form-group ">
                                <label for="firstName" class="col-sm-5 control-label">Enrollment ID:</label>
                                <div class="col-sm-3">
                                    @ViewBag.EnrollmentID

                                </div>
                                <div class="col-sm-4"></div>
                            </div>
                            <div class="form-group ">
                                <label for="middleName" class="col-sm-5 control-label">Enrollment Response Status:</label>
                                <div class="col-sm-3">
                                    @ViewBag.EnrollmentResponse

                                </div>
                                <div class="col-sm-4"></div>
                            </div>
                        }
                        else
                        {
                            <div class="form-group ">
                                <label for="lastName" class="col-sm-5 control-label">Error Response:</label>
                                <div class="col-sm-3">

                                    <table class="table table-bordered">
                                        <thead>
                                            <tr>
                                                <th>Code</th>
                                                <th>Detail</th>
                                                <th>Status</th>
                                            </tr>
                                        </thead>
                                        <tbody>


                                            @{ if (ViewBag.errorobj != null)
                                                {
                                                    MEdicareEnrollement.Controllers.ResultController.ErrorResponse Obj = ViewBag.errorobj;
                                                    foreach (var item in Obj.errors)
                                                    {
                                                        <tr>
                                                            <td>@item.code</td>
                                                            <td>@item.detail</td>
                                                            <td>@item.status</td>
                                                        </tr>
                                                    }
                                                }
                                            }

                                        </tbody>
                                    </table>
                                </div>
                                <div class="col-sm-4"></div>
                            </div>
                                                }}
                </form>
            </div>
            <div>
                <a class="btn btn-primary" href="~/Enrollment/CurrentEnrol">Dashboard</a>
            </div>
        </div>

    </div>
</div>