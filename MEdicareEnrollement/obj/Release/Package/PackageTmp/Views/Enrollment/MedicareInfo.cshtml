
@{
    ViewBag.Title = "MedicareInfo";
    Layout = "~/Views/Shared/_MedicareLayout.cshtml";
}

<div data-ng-app="MedicareEnrollmentModule" ng-controller="MCIController">
    <div class="main-content" style="min-height: 0vh;">
        <div class="container">
            <div class="form-head">
                Section 2: Medicare Information
            </div>
            <div><label for="firstName" class="control-label">Plan:{{PLAName}}</label></div>
            <div><label for="firstName" class="control-label">Plan year:2017</label></div>
        </div>

        <center><p>To complete this section, refer to your Medicare card. Please fill in the information exactly as it appears on your card.</p></center>
        <form id="sec2_form">
            <div class="form-qa">
                <div class="form-group required col-sm-12">
                    <label for="firstName" class="col-sm-5 control-label">First Name:</label>
                    <div class="col-sm-3">
                        <input name="first_name" type="text" id="firstName" ng-model="firstname" placeholder="First Name" class="form-control">
                    </div>
                    <div class="col-sm-4"></div>
                </div>
                <div class="form-group col-sm-12">
                    <label for="middleName" class="col-sm-5 control-label">Middle Name:</label>
                    <div class="col-sm-3">
                        <input type="text" id="middleName" ng-model="middleName" placeholder="Middle Name" class="form-control">
                    </div>
                    <div class="col-sm-4"></div>
                </div>
                <div class="form-group required col-sm-12">
                    <label for="lastName" class="col-sm-5 control-label">Last Name:</label>
                    <div class="col-sm-3">
                        <input name="last_name" type="text" id="lastName" ng-model="lastName" placeholder="Last Name" class="form-control">
                    </div>
                    <div class="col-sm-4"></div>
                </div>
                <div class="form-group required col-sm-12">
                    <label class="control-label col-sm-5">Gender:</label>
                    <div class="col-sm-3">
                        <div class="" style="text-align:left">
                            <div class="col-sm-6" style="padding-left:0;">
                                <label class="radio-inline">
                                    <input name="gender" type="radio" id="maleRadio" ng-model="Gender" value="Male">Male
                                </label>
                            </div>
                            <div class="col-sm-6" style="padding-left:0;">
                                <label class="radio-inline">
                                    <input name="gender" type="radio" id="maleRadio" ng-model="Gender" value="Female">Female
                                </label>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4"></div>
                </div> <!-- /.form-group -->
                <div class="form-group required col-sm-12">
                    <label for="stateofResidency" class="col-sm-5 control-label">State of Permanent Residence:</label>
                    <div class="col-sm-3">
                        @*
                            <input type="text" id="State" ng-model="state" placeholder="State" class="form-control">*@
                        @*<select id="State" class="form-control" ng-model="state">
                                <option>AL</option>
                                <option>Select One</option>
                                <option>Select One</option>
                                <option>Select One</option>
                                <option>Select One</option>
                                <option>Select One</option>
                                <option>Select One</option>
                                <option>Select One</option>
                            </select>*@
                        <select name="state" id="ddlUser" ng-int="StateName = 'Select'" ng-model="state" ng-options="itemAccoun.StateAbbreviation as itemAccoun.StateName for itemAccoun in getStateData" ng-change="StateSelect()" class="textbox">
                            <option value="">Select State</option>
                        </select>
                    </div>
                    <div class="col-sm-4"></div>
                </div>
                <div class="form-group required col-sm-12">
                    <label for="dateofbirth" class="col-sm-5 control-label">Date of Birth:</label>
                    <div class="col-sm-3" style="padding-left: 0;">
                        <div class="col-sm-4">
                            <input name="date1" type="text" id="lastName" ng-model="domonth" placeholder="MM" class="form-control" maxlength="2">
                        </div>
                        <div class="col-sm-4">
                            <input name="date2" type="text" id="lastName" placeholder="DD" ng-model="doday" class="form-control" maxlength="2">
                        </div>
                        <div class="col-sm-4">
                            <input name="date3" type="text" id="lastName" placeholder="YYYY" ng-model="doyear" class="form-control" maxlength="4">
                        </div>
                    </div>
                    <div class="col-sm-4"></div>
                </div>
            </div>
        </form>
        <div style="clear: both;"></div>
        <div class="col-sm-12">
            <div class="col-sm-2"></div>
            <div class="col-md-8" style="text-align: center;">
                @*<a href="~/Enrollment/Index" class="btn btn-default">Previous</a>
                    <a href="~/Enrollment/ContactInfo" class="btn btn-default">Next</a>*@
                @*<button type="button" class="btn btn-primary" id="SaveMCI" ng-click="InsertMCI()">Save</button>*@
            </div>
            <div class="col-sm-2"></div>
        </div>
    </div>

    <hr />

