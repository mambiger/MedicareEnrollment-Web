<script>
    $(document).ready(function () {

        $('#sec7_fomr44').bootstrapValidator({
            // To use feedback icons, ensure that you use Bootstrap v3.1.0 or later
            feedbackIcons: {
                valid: 'glyphicon glyphicon-ok',
                invalid: 'glyphicon glyphicon-remove',
                validating: 'glyphicon glyphicon-refresh'
            },
            fields: {
                instiname: {
                    validators: {

                        notEmpty: {
                            message: 'Please enter Institution Name'
                        }
                    }
                },


                instiaddress: {
                    validators: {
                        stringLength: {
                            min: 6,
                        },
                        notEmpty: {
                            message: 'Please enter Institution Address1'
                        }
                    }
                },

                insticity: {
                    validators: {
                        stringLength: {
                            min: 4,
                        },
                        notEmpty: {
                            message: 'Please enter Institution City'
                        }
                    }
                },
                instistate: {
                    validators: {
                        notEmpty: {
                            message: 'Please select Institution State'
                        }
                    }
                },
                instizip: {
                    validators: {
                        notEmpty: {
                            message: 'Please enter Institution Zip'
                        },
                        zipCode: {
                            country: 'US',
                            message: 'The value is not valid US postal code'
                        },
                    }
                },

                firstname: {
                    validators: {

                        notEmpty: {
                            message: 'Please enter First Name'
                        }
                    }
                },
                lastname: {
                    validators: {

                        notEmpty: {
                            message: 'Please enter Last Name'
                        }
                    }
                },
                relation: {
                    validators: {

                        notEmpty: {
                            message: 'Please enter Relation'
                        }
                    }
                },
                date1: {
                    validators: {
                        notEmpty: {
                            message: 'Please enter Month'
                        },
                    }
                },
                date2: {
                    validators: {
                        notEmpty: {
                            message: 'Please enter Date'
                        },
                    }
                },
                date3: {
                    validators: {
                        stringLength: {
                            min: 4,
                            message: 'Enter Valid Number'
                        },
                        notEmpty: {
                            message: 'Please enter Year'
                        },
                    }
                },
                }
        })
            .on('success.form.bv', function (e) {
                $('#success_message').slideDown({ opacity: "show" }, "slow") // Do something ...
                $('#contact_form').data('bootstrapValidator').resetForm();

                // Prevent form submission
                e.preventDefault();

                // Get the form instance
                var $form = $(e.target);

                // Get the BootstrapValidator instance
                var bv = $form.data('bootstrapValidator');

                // Use Ajax to submit form data
                $.post($form.attr('action'), $form.serialize(), function (result) {
                    console.log(result);
                }, 'json');
            });
    });
</script>



