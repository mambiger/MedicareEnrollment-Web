        </div>
        <div class="form-group">
            <div class="col-md-12 col-sm-12 col-lg-12">

                <center>  <button form="reg_form1" id="btnnext" type="submit" class="btn btn-default" ng-click="InsertRegistrationInfo()">Register</button></center>

            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/Scripts/CommonModule.js"></script>
<script src="~/Scripts/Scripts/Registration/RegistrationControllerV1.js"></script>
<script src="~/Scripts/Scripts/Registration/RegistrationService.js"></script>
<script src="~/Content/regValidator.js"></script>

<script>
    $(document).ready(function () {
        $("#btnnext").click(function () {
            $('#reg_form1').bootstrapValidator('validate');
        });
        ValidateIt();
    });

</script>



