                        <div class="form-group required">
                            <label for="lastName" class="col-sm-5 control-label">Agent Email:</label>
                            <div class="col-sm-5">
                                <input name="email" type="text" id="lastName" class="form-control" ng-model="Email">
                            </div>
                            <div class="col-sm-2"></div>
                        </div>
                        <div class="form-group required">
                            <label for="lastName" class="col-sm-5 control-label">Confirm Email:</label>
                            <div class="col-sm-5">
                                <input name="confirmemail" type="text" id="lastName" class="form-control" ng-model="ConfirmEmail">
                            </div>
                            <div class="col-sm-2"></div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group required">
                            <label for="lastName" class="col-sm-5 control-label">Password:</label>
                            <div class="col-sm-5">
                                <input name="password" type="password" id="lastName" class="form-control" ng-model="Password">
                                <div class="col-sm-2"></div>
                            </div>
                        </div>
                        <div class="form-group required">
                            <label for="lastName" class="col-sm-5 control-label">Confirm Password:</label>
                            <div class="col-sm-5">
                                <input name="confirmpassword" type="password" id="lastName" class="form-control">
                            </div>
                            <div class="col-sm-2"></div>
                        </div>
                    </div>
                </div>
