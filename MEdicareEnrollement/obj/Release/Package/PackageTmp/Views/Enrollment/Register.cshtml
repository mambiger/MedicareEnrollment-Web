                            <label for="middleName" class="col-sm-5 control-label ">Affinity ID</label>
                            <div class="col-sm-6">
                                <select name="affinityid" id="affinityid" ng-model="AffinityID" ng-init="AffinityID='0'" class="form-control">
                                    <option value="1">Educator Group Plans Insurance</option>
                                </select>
