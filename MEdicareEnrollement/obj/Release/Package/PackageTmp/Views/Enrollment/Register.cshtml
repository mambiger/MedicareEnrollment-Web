                            <div class="form-group required">
                                <label for="middleName" class="col-sm-5 control-label">Company Name:</label>
                                <div class="col-sm-5">
                                    <input type="text" id="middleName" class="form-control" ng-model="CompName">
                                </div>
                                <div class="col-sm-2"></div>
