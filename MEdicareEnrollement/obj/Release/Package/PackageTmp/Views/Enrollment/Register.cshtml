                        <div class="form-group required">
                            <label for="lastName" class="col-sm-5 control-label">Agent First Name:</label>
                            <div class="col-sm-5">
                                <input name="firstname" type="text" id="lastName" class="form-control" ng-model="FirstName">
                            </div>
                            <div class="col-sm-2"></div>
                        </div>
                        <div class="form-group required">
                            <label for="lastName" class="col-sm-5 control-label">Middle Name:</label>
                            <div class="col-sm-5">
                                <input name="middleName" type="text" id="lastName" class="form-control" ng-model="MiddleName">
                            </div>
                            <div class="col-sm-2"></div>
                        </div>
                        <div class="form-group required">
                            <label for="lastName" class="col-sm-5 control-label">Agent Last Name:</label>
                            <div class="col-sm-5">
                                <input name="lastName" type="text" id="lastName" class="form-control" ng-model="LastName">
                            </div>
                            <div class="col-sm-2"></div>
                        </div>
                        <div class="form-group required">
                            <label for="lastName" class="col-sm-5 control-label">Agent SAN:</label>
                            <div class="col-sm-5">
                                <input name="agentsan" type="text" id="lastName" class="form-control" ng-model="SAN">
                            </div>
                            <div class="col-sm-2"></div>
                        </div>

                    </div>
                    <div class="col-sm-6">
                        @*<div class="form-group required">
                                <label for="lastName" class="col-sm-5 control-label">Mailling Address:</label>
                                <div class="col-sm-5">
                                    <input type="text" id="lastName" class="form-control" ng-model="Address">
                                </div>
                                <div class="col-sm-2"></div>
                            </div>
                            <div class="form-group required">
                                <label for="lastName" class="col-sm-5 control-label">City:</label>
                                <div class="col-sm-5">
                                    <input type="text" id="lastName" class="form-control" ng-model="City">
                                </div>
                                <div class="col-sm-2"></div>
                            </div>
                            <div class="form-group required">
                                <label for="lastName" class="col-sm-5 control-label">State:</label>
                                <div class="col-sm-5">
