

@{
    ViewBag.Title = "CurrentEnrol";
    Layout = "~/Views/Shared/_MedicareLayout.cshtml";
}
<div class="main-content" data-ng-app="MedicareEnrollmentModule" id="divEnrollment" ng-controller="EnrollmentDetailController">
    <div class="container">
        <div class="caption-heading">
            <div class="caption-h">
                <div class="radio">
                    <label><input type="radio" name="optradio" checked>Today</label>
                </div>
                <div class="radio">
                    <label><input type="radio" name="optradio">Last 7 Days</label>
                </div>
            </div>
        </div>
    </div>

    <div class="container" style="margin-top: 30px; margin-bottom: 30px;">
        <table class="table table-hover" id="Enrollmentdetailid">
            <thead>
                <tr>
                    <th>Client Name</th>
                    <th>Mailing Address</th>
                    <th>Date Submitted</th>
                    <th>Effective Date</th>
                    <th>Status</th>
                    <th>Action</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
    <div style="position: relative; left: 15px; top: 10px;">
        <button class="btn btn-info" ng-click="AddnewEnrollment()"><i class="fa fa-plus" aria-hidden="true"></i> Add Enrollment</button>
    </div>
</div>
<script src="~/Scripts/Scripts/EnrollmentDetails/EnrollmentDetailServiceV1.js"></script>
<script src="~/Scripts/Scripts/EnrollmentDetails/EnrollmentDetailControllerV4.js"></script>
<script type="text/javascript">


    function edit(applicantid, applicationdetailid) {
        var scope = angular.element(document.getElementById("divEnrollment")).scope();
        scope.$apply(function () {
            scope.EditNavigate(applicantid, applicationdetailid);
        });
    }
    function deleteenroll(applicantid, applicationdetailid) {
        if (confirm("Are you sure, do you want to delete?")) {
            var scope = angular.element(document.getElementById("divEnrollment")).scope();
            scope.$apply(function () {
                debugger;
                scope.DeleteNavigate(applicantid, applicationdetailid);
            });
        }
        return false;

    }
</script>






