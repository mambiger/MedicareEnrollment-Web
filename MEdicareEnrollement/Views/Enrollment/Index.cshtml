<script>
    $(document).ready(function () {
        $('#sec1_form').bootstrapValidator({
            // To use feedback icons, ensure that you use Bootstrap v3.1.0 or later
            feedbackIcons: {
                valid: 'glyphicon glyphicon-ok',
                invalid: 'glyphicon glyphicon-remove',
                validating: 'glyphicon glyphicon-refresh'
            },
            fields: {
                selectplan: {
                    validators: {

                        notEmpty: {
                            message: 'Please Select Plan'
                        }
                    }
                },
                first_name: {
                    validators: {

                        notEmpty: {
                            message: 'Please enter POA First Name'
                        }
                    }
                },
                last_name: {
                    validators: {

                        notEmpty: {
                            message: 'Please enter POA Last Name'
                        }
                    }
                },
                address1: {
                    validators: {
                        
                        notEmpty: {
                            message: 'Please enter POA Address 1'
                        }
                    }
                },

                city: {
                    validators: {
                        
                        notEmpty: {
                            message: 'Please enter POA City'
                        }
                    }
                },
                state: {
                    validators: {
                        notEmpty: {
                            message: 'Please select POA State'
                        }
                    }
                },
                zip: {
                    validators: {
                        notEmpty: {
                            message: 'Please enter POA Zip Code'
                        },
                        zipCode: {
                            country: 'US',
                            message: 'The value is not valid US postal code'
                        },
                    }
                },


                phone3: {
                    validators: {
                        stringLength: {
                            min: 4,
                            message: 'Enter valid number'
                        },
                        notEmpty: {
                            message: 'Please enter Phone Number'
                        },
                    }
                },
                relationship: {
                    validators: {
                        notEmpty: {
                            message: 'Please enter POA Relationship to Applicant'
                        }
                    }
                }
            }
        })
         .on('success.field.fv', function (e, data) {
             if (data.fv.getSubmitButton()) {
                 data.fv.disableSubmitButtons(false);
             }
         });


    });


</script>
<script>





    function isNumberKey(evt) {

        var charCode = (evt.which) ? evt.which : event.keyCode
